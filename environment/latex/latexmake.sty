% latexmake.sty
% This package supports compatibility of figure inclusion 
% for both latex and pdflatex in a single document
% Mike Buksas and Brendt Wohlberg
% Most recent modification: 25 August 2000

\newif\ifpdf
\ifx\pdfoutput\undefined
  \pdffalse
\else
  \pdfoutput=1
  \pdftrue
\fi

\ifpdf
  \usepackage{graphicx}
  \DeclareGraphicsExtensions{.pdf}
  \pdfcompresslevel=9
\else
  \usepackage{graphicx}
  \DeclareGraphicsExtensions{.pstex,.ps,.eps}
\fi
