#%Module1.0#####################################################################
##
## modules modulefile
##
## modulefiles/modules.  Generated from modules.in by configure.
##

# global control file
if { [file exists $env(MODULESHOME)/etc/control] } {
   source $env(MODULESHOME)/etc/control;
}

# local variables
set prefix      /usr/projects/draco/vendors/cvs
#set libdir      $prefix/lib
#set incdir      $prefix/include
set bindir      $prefix/bin
#set mandir      $prefix/man

# module whatis
module-whatis   "CVS";

# module help
proc ModulesHelp { } {
   global version prefix bindir;

   puts stderr "\tCVS\n";
#   puts stderr "\tGSL_LIB_DIR\t= $libdir";
#   puts stderr "\tGSL_INC_DIR\t= $incdir";
   puts stderr "\tPATH+= $bindir";
#   puts stderr "\tMANPATH+= $mandir";
}

if { ![file exists $prefix] } {
   puts stderr "\n[module-info name]: $prefix: No such file or directory.\n";
   break;
   exit 1;
}

prepend-path    PATH            $bindir;
#prepend-path    MANPATH         $mandir;

#setenv GSL_LIB_DIR $libdir
#setenv GSL_INC_DIR $incdir


