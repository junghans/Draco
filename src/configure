# Check whether --with-mpi or --without-mpi was given.
if test "${with_mpi+set}" = set; then
  withval="$with_mpi"

fi;





# Check whether --with-mpi-inc or --without-mpi-inc was given.
if test "${with_mpi_inc+set}" = set; then
  withval="$with_mpi_inc"
  if test $with_mpi_inc != "no" ; then
      if test $with_mpi_inc = "yes" ; then
         # following eval needed to remove possible '\' from \${MPI_INC_DIR}
         eval with_mpi_inc=\${MPI_INC_DIR}
      fi

      # this command removes double slashes and any trailing slash

      with_mpi_inc=`eval echo "$with_mpi_inc" | sed 's%//*%/%g' | sed 's%/$%%'`
      if test "$with_mpi_inc:-null}" = "null" ; then
         { echo "configure: error: --with-mpi-inc directory is unset" 1>&2; \
           exit 1; }
      fi
      if test ! -d $with_mpi_inc ; then
         { echo "configure: error: $with_mpi_inc: invalid directory" 1>&2; \
           exit 1; }
      fi

      # this sets up the shell variable, with the name of the CPPtoken,
      # and that we later will do an AC_SUBST on.
      MPI_INC="${with_mpi_inc}/"

      # this defines the CPP macro with the directory and single slash appended.
      cat >>confdefs.h <<_ACEOF
#define MPI_INC ${with_mpi_inc}/
_ACEOF

      # print a message to the users (that can be turned off with --silent)

      echo "MPI_INC has been set to $MPI_INC" 1>&6

   fi
fi;







